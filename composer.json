{
  "name": "erdincesendemir/laravel-paytr",
  "description": "PayTR payment gateway integration for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Erdin√ß Esendemir",
      "email": "neterdinc@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0",
